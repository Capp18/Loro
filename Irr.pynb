import numpy as np
import networkx as nx
n=100
g = nx.erdos_renyi_graph(n, 0.3, seed=123, directed=False)
A = nx.to_numpy_matrix(g,dtype=float)

S=A+np.identity(n,int)

for i in range(2,n-1):
    S+=np.linalg.matrix_power(A,i)

if np.count_nonzero(S)==n*n:
    print('The adjacency matrix A is irreducible')
